#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/behaviors.h>
#include <dt-bindings/zmk/pointing.h>
#include <locale/keys_hu.h>

#define BASE 0
#define SYMNUM 1
#define NAVCTRL  2
#define FNBT 3
#define MOUSE 4
#define BASE_IPAD 5
#define SYMNUM_IPAD 6
#define NAVCTRL_IPAD 7
#define FNBT_IPAD 8
#define MOUSE_IPAD 9

#define HU_LDQT (RA(HU_O))
#define HU_RDQT (RA(HU_P))
#define HU_EMDASH (RA(HU_Z))

#define HU_IPAD_PIPE (RA(HU_I_ACUTE))
#define HU_IPAD_CARET (RA(HU_A_ACUTE))
#define HU_IPAD_TILDE (RA(HU_U_ACUTE))
#define HU_IPAD_GRAVE (RA(HU_U_DOUBLE_ACUTE))
#define HU_IPAD_BACKSLASH (RA(HU_U_UMLAUT))
#define HU_IPAD_LDQT (RA(HU_V))
#define HU_IPAD_DOLLAR (RA(HU_N4))
#define HU_IPAD_HASH (RA(LS(HU_N3)))
#define HU_IPAD_AMPS (RA(HU_N1))
#define HU_IPAD_AT (RA(HU_Q))
#define HU_IPAD_LBRC (RA(HU_N7))
#define HU_IPAD_RBRC (RA(HU_O_UMLAUT))
#define HU_IPAD_LT (RA(LS(HU_Y)))
#define HU_IPAD_GT (RA(LS(HU_X)))
#define HU_IPAD_ASTERISK (RA(LS(HU_COMMA)))
#define HU_IPAD_RDQT (RA(HU_B))
#define HU_IPAD_EMDASH (RA(HU_MINUS))

#define KEYS_L 0 1 2 3 4 10 11 12 13 14 20 21 22 23 24 25 // Left-hand keys.
#define KEYS_R 5 6 7 8 9 15 16 17 18 19 20 26 27 28 29 30 31 // Right-hand keys.
#define THUMBS 32 33 34 35 36 37 // Thumb keys.

/ {
	behaviors {
		hml: home_row_mod_left {
			compatible = "zmk,behavior-hold-tap";
			#binding-cells = <2>;
			flavor = "balanced";
			require-prior-idle-ms = <150>;
			tapping-term-ms = <280>;
			quick-tap-ms = <175>;
			bindings = <&kp>, <&kp>;
			hold-trigger-key-positions = <KEYS_R THUMBS>;
			hold-trigger-on-release;
		};
		hmr: home_row_mod_right {
			compatible = "zmk,behavior-hold-tap";
			#binding-cells = <2>;
			flavor = "balanced";
			require-prior-idle-ms = <150>;
			tapping-term-ms = <280>;
			quick-tap-ms = <175>;
			bindings = <&kp>, <&kp>;
			hold-trigger-key-positions = <KEYS_L THUMBS>;
			hold-trigger-on-release;
		};
        td0: tap_dance_0 {
            compatible = "zmk,behavior-tap-dance";
            #binding-cells = <0>;
            tapping-term-ms = <200>;
            bindings = <&kp LSHIFT>, <&caps_word>, <&kp CAPSLOCK>;
        };
	};
	
	combos {
		compatible = "zmk,combos";
		// Bspc and Del
		combo_backspace {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <6 7>;
			bindings = <&kp BACKSPACE>;
		};
		combo_delete {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <7 8>;
			bindings = <&kp DEL>;
		};
		
        // Symbols
		combo_single_quote {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <1 2>;
			bindings = <&kp HU_SQT>;
		};
		combo_double_quote {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <2 3>;
			bindings = <&kp HU_DQT>;
		};
		combo_plus {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <3 4>;
			bindings = <&kp HU_PLUS>;
		};

		combo_backslash {
			timeout-ms = <50>;
			layers = <BASE>;
			key-positions = <13 14>;
			bindings = <&kp HU_BACKSLASH>;
		};
		combo_backslash_ipad {
			timeout-ms = <50>;
			layers = <BASE_IPAD>;
			key-positions = <13 14>;
			bindings = <&kp HU_IPAD_BACKSLASH>;
		};

		combo_slash {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <15 16>;
			bindings = <&kp HU_SLASH>;
		};

		combo_equal {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <21 22>;
			bindings = <&kp HU_EQUAL>;
		};
		combo_left_parenthesis {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <22 23>;
			bindings = <&kp HU_LPAR>;
		};
		combo_right_parenthesis {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <23 24>;
			bindings = <&kp HU_RPAR>;
		};
		combo_at {
			timeout-ms = <50>;
			layers = <BASE>;
			key-positions = <24 25>;
			bindings = <&kp HU_AT>;
		};
		combo_at_ipad {
			timeout-ms = <50>;
			layers = <BASE_IPAD>;
			key-positions = <24 25>;
			bindings = <&kp HU_IPAD_AT>;
		};

        combo_emdash {
			timeout-ms = <50>;
			layers = <BASE>;
			key-positions = <26 27>;
			bindings = <&kp HU_EMDASH>;
		};
		combo_emdash_ipad {
			timeout-ms = <50>;
			layers = <BASE_IPAD>;
			key-positions = <26 27>;
			bindings = <&kp HU_IPAD_EMDASH>;
		};
		combo_exclamation {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <27 28>;
			bindings = <&kp HU_EXCL>;
		};
		combo_semicolon {
			timeout-ms = <50>;
			layers = <BASE BASE_IPAD>;
			key-positions = <28 29>;
			bindings = <&kp HU_SEMI>;
		};
		combo_asterisk {
			timeout-ms = <50>;
			layers = <BASE>;
			key-positions = <29 30>;
			bindings = <&kp HU_ASTERISK>;
		};
		combo_asterisk_ipad {
			timeout-ms = <50>;
			layers = <BASE_IPAD>;
			key-positions = <29 30>;
			bindings = <&kp HU_IPAD_ASTERISK>;
		};
		
		// Word start and end
		combo_word_start {
			timeout-ms = <50>;
			layers = <NAVCTRL>;
			key-positions = <27 28>;
			bindings = <&kp LC(LEFT)>;
		};
		combo_word_end {
			timeout-ms = <50>;
			layers = <NAVCTRL>;
			key-positions = <28 29>;
			bindings = <&kp LC(RIGHT)>;
		};
		combo_word_start_ipad {
			timeout-ms = <50>;
			layers = <NAVCTRL_IPAD>;
			key-positions = <27 28>;
			bindings = <&kp LA(LEFT)>;
		};
		combo_word_end_ipad {
			timeout-ms = <50>;
			layers = <NAVCTRL_IPAD>;
			key-positions = <28 29>;
			bindings = <&kp LA(RIGHT)>;
		};
    	};

    	macros {
		teams_leave: teams_leave {
		    label = "teams_leave";
		    compatible = "zmk,behavior-macro";
		    #binding-cells = <0>;
		    bindings
		        = <&macro_tap &kp LG(HU_N5)>
		        , <&macro_tap &kp LC(LS(HU_H))>
		        ;
		};
		teams_sound: teams_sound {
		    label = "teams_sound";
		    compatible = "zmk,behavior-macro";
		    #binding-cells = <0>;
		    bindings
		        = <&macro_tap &kp LG(HU_N5)>
		        , <&macro_tap &kp LC(LS(HU_M))>
		        ;
		};
		teams_camera: teams_camera {
		    label = "teams_camera";
		    compatible = "zmk,behavior-macro";
		    #binding-cells = <0>;
		    bindings
		        = <&macro_tap &kp LG(HU_N5)>
		        , <&macro_tap &kp LC(LS(HU_O))>
		        ;
		};
        ipad_profile: ipad_profile {
		    label = "ipad_profile";
		    compatible = "zmk,behavior-macro";
		    #binding-cells = <0>;
		    bindings
		        = <&bt BT_SEL 0>
                , <&to BASE_IPAD>
		        ;
		};
        linux_profile: linux_profile {
		    label = "linux_profile";
		    compatible = "zmk,behavior-macro";
		    #binding-cells = <0>;
		    bindings
		        = <&bt BT_SEL 1>
                , <&to BASE>
		        ;
		};
        mac_profile: mac_profile {
		    label = "mac_profile";
		    compatible = "zmk,behavior-macro";
		    #binding-cells = <0>;
		    bindings
		        = <&bt BT_SEL 2>
                , <&to BASE_IPAD>
		        ;
		};
        windows_profile: windows_profile {
		    label = "windows_profile";
		    compatible = "zmk,behavior-macro";
		    #binding-cells = <0>;
		    bindings
		        = <&bt BT_SEL 3>
                , <&to BASE>
		        ;
		};
        windows_profile_2: windows_profile_2 {
		    label = "windows_profile_2";
		    compatible = "zmk,behavior-macro";
		    #binding-cells = <0>;
		    bindings
		        = <&bt BT_SEL 4>
                , <&to BASE>
		        ;
		};
	};

	keymap {
		compatible = "zmk,keymap";
		
		base_layer {
			label= "BASE";
			bindings = <
			&kp HU_Q &kp HU_W &kp HU_F &kp HU_P &kp HU_B &kp HU_J &kp HU_L &kp HU_U &kp HU_Z &kp HU_O_UMLAUT
			&kp HU_A &hml LGUI HU_R &hml LALT HU_S &hml LCTRL HU_T &kp HU_G &kp HU_M &hmr LCTRL HU_N &hmr LALT HU_E &hmr LGUI HU_I &kp HU_O
			&kp TAB &kp HU_Y &kp HU_X &kp HU_C &kp HU_D &kp HU_V &kp HU_H &kp HU_K &kp HU_COMMA &kp HU_DOT &kp HU_MINUS &kp HU_U_UMLAUT
			&lt MOUSE ESC &mo SYMNUM &lt FNBT ENTER &kp SPACE &td0 &mo NAVCTRL
		        >;
		};
                
        symnum_layer {
			label= "SYMNUM";
			bindings = <
			&kp HU_SECT &kp HU_N1 &kp HU_N2 &kp HU_N3 &kp HU_PERCENT &kp HU_LT &kp HU_GT &kp HU_U_ACUTE &kp HU_CARET &kp HU_O_DOUBLE_ACUTE
			&kp HU_A_ACUTE &hml LGUI HU_N4 &hml LALT HU_N5 &hml LCTRL HU_N6 &kp HU_N0 &kp HU_LBKT &hmr LCTRL HU_RBKT &hmr LALT HU_E_ACUTE &hmr LGUI HU_I_ACUTE &kp HU_O_ACUTE
			&kp HU_GRAVE &kp HU_HASH &kp HU_N7 &kp HU_N8 &kp HU_N9 &kp HU_TILDE &kp HU_LBRC &kp HU_RBRC &kp HU_AMPS &kp HU_PIPE &kp HU_DOLLAR &kp HU_U_DOUBLE_ACUTE
			&none &trans &none &trans &trans &kp RALT
                        >;
                };
		
	    navctrl_layer {
			label= "NAVCTRL";
			bindings = <
			&none &kp C_PREV &kp C_PLAY_PAUSE &kp C_NEXT &kp C_VOL_UP &kp C_BRI_UP &kp BACKSPACE &kp DEL &kp INS &none
			&none &kp LGUI &kp LALT &kp LCTRL &kp C_VOL_DN &kp C_BRI_DN &kp LEFT &kp DOWN &kp UP &kp RIGHT
			&kp LC(HU_Y) &kp LC(HU_Z) &kp LC(HU_X) &kp LC(HU_C) &kp LC(HU_V) &kp C_MUTE &kp PRINTSCREEN &kp HOME &kp PG_DN &kp PG_UP &kp END &to BASE_IPAD
			&tog NAVCTRL &td0 &kp ENTER &none &none &trans
                        >;
                };
                
        fnbt_layer {
			label= "FNBT";
			bindings = <
			&bt BT_DISC 0 &bt BT_DISC 1 &bt BT_DISC 2 &bt BT_DISC 3 &bt BT_DISC 4 &none &kp F1 &kp F2 &kp F3 &kp F4
			&none &kp LGUI &kp LALT &kp LCTRL &bt BT_CLR &none &kp F5 &kp F6 &kp F7 &kp F8
			&bt BT_PRV &ipad_profile &linux_profile &mac_profile &windows_profile &windows_profile_2 &none &kp F9 &kp F10 &kp F11 &kp F12 &bt BT_NXT
			&trans &trans &trans &trans &trans &sk RALT
                        >;
                };

        mouse_layer {
			label= "MOUSE";
			bindings = <
			&none &none &none &none &none &none &none &none &none &none
			&msc SCRL_LEFT &msc SCRL_DOWN &msc SCRL_UP &msc SCRL_RIGHT &none &none &mmv MOVE_LEFT &mmv MOVE_DOWN &mmv MOVE_UP &mmv MOVE_RIGHT
			&none &teams_leave &teams_sound &teams_camera &none &kp LSHIFT &kp LCTRL &kp LALT &kp LGUI &none &none
			&trans &none &none &mkp RCLK &mkp LCLK &tog MOUSE
                        >;
                };
		
		base_ipad_layer {
			label= "BASE_IPAD";
			bindings = <
			&trans &trans &trans &trans &trans &trans &trans &trans &trans &trans
			&hml GLOBE HU_A &hml LCTRL HU_R &trans &hml LGUI HU_T &trans &trans &hmr LGUI HU_N &trans &hmr LCTRL HU_I &hmr GLOBE HU_O
			&trans &trans &trans &trans &trans &trans &trans &trans &trans &trans &trans &trans
			&lt MOUSE_IPAD ESC &mo SYMNUM_IPAD &lt FNBT_IPAD ENTER &trans &trans &mo NAVCTRL_IPAD
                        >;
                };
                
        symnum_ipad_layer {
			label= "SYMNUM_IPAD";
			bindings = <
			&kp HU_SECT &kp HU_N1 &kp HU_N2 &kp HU_N3 &kp HU_PERCENT &kp HU_IPAD_LT &kp HU_IPAD_GT &kp HU_U_ACUTE &kp HU_IPAD_CARET &kp HU_O_DOUBLE_ACUTE
			&hml GLOBE HU_A_ACUTE &hml LCTRL HU_N4 &hml LALT HU_N5 &hml LGUI HU_N6 &kp HU_I_ACUTE &kp HU_LBKT &hmr LGUI HU_RBKT &hmr LALT HU_E_ACUTE &hmr LCTRL HU_N0 &hmr GLOBE HU_O_ACUTE
			&kp HU_IPAD_GRAVE &kp HU_IPAD_HASH &kp HU_N7 &kp HU_N8 &kp HU_N9 &kp HU_IPAD_TILDE &kp HU_IPAD_LBRC &kp HU_IPAD_RBRC &kp HU_IPAD_AMPS &kp HU_IPAD_PIPE &kp HU_IPAD_DOLLAR &kp HU_U_DOUBLE_ACUTE
			&none &trans &none &trans &trans &sk RALT
                        >;
                };
                
        navctrl_ipad_layer {
			label= "NAVCTRL_IPAD";
			bindings = <
			&none &kp C_PREV &kp C_PLAY_PAUSE &kp C_NEXT &kp C_VOL_UP &kp C_BRI_UP &kp BACKSPACE &kp DEL &kp INS &none
			&kp GLOBE &kp LCTRL &kp LALT &kp LGUI &kp C_VOL_DN &kp C_BRI_DN &kp LEFT &kp DOWN &kp UP &kp RIGHT
			&kp LS(LG(HU_Z)) &kp LG(HU_Z) &kp LG(HU_X) &kp LG(HU_C) &kp LG(HU_V) &kp C_MUTE &kp PRINTSCREEN &kp LC(LEFT) &kp PG_DN &kp PG_UP &kp LC(RIGHT) &to BASE
			&tog NAVCTRL_IPAD &td0 &kp ENTER &none &none &trans
                        >;
                };
                
        fnbt_ipad_layer {
			label= "FNBT_IPAD";
			bindings = <
			&bt BT_DISC 0 &bt BT_DISC 1 &bt BT_DISC 2 &bt BT_DISC 3 &bt BT_DISC 4 &none &kp F1 &kp F2 &kp F3 &kp F4
			&kp GLOBE &kp LCTRL &kp LALT &kp LGUI &bt BT_CLR &none &kp F5 &kp F6 &kp F7 &kp F8
			&bt BT_PRV &ipad_profile &linux_profile &mac_profile &windows_profile &windows_profile_2 &none &kp F9 &kp F10 &kp F11 &kp F12 &bt BT_NXT
			&trans &trans &trans &trans &trans &sk RALT
                        >;
                };
                
        mouse_ipad_layer {
			label= "MOUSE_IPAD";
			bindings = <
			&none &none &none &none &none &none &none &none &none &none
			&msc SCRL_LEFT &msc SCRL_DOWN &msc SCRL_UP &msc SCRL_RIGHT &none &none &mmv MOVE_LEFT &mmv MOVE_DOWN &mmv MOVE_UP &mmv MOVE_RIGHT
			&none &teams_leave &teams_sound &teams_camera &none &kp LSHIFT &kp LGUI &kp LALT &kp LCTRL &kp GLOBE &none
			&trans &none &none &mkp RCLK &mkp LCLK &tog MOUSE_IPAD
                        >;
                };
        };
};
